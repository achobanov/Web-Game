{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": { "node": "10" }
      }
    ],
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    "@babel/plugin-transform-object-super",
    "syntax-async-functions"
  ]
}
